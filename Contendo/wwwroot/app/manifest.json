{
    "_version": "1.2.0",
    "sap.app": {
        "_version": "1.2.0",
        "id": "vcs",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "VCS UI",
        "description": "{{appDescription}}",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "dataSources": {
        }
    },
    "sap.ui": {
        "_version": "1.2.0",
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://task",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_hcb",
            "sap_bluecrystal"
        ]
    },
    "sap.ui5": {
        "_version": "1.2.0",
        "handleValidation": true,

        "rootView": {
            "viewName": "vcs.view.FullApp",
            "type": "XML",
            "id": "fullApp"
        },
        "dependencies": {
            "minUI5Version": "1.36.0",
            "libs": {
                "sap.ui.core": {
                    "minVersion": "1.36.0"
                },
                "sap.m": {
                    "minVersion": "1.36.0"
                }
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "vcs.i18n.i18n"
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "style/site.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "vcs.view",
                "controlId": "fullApp",
                "controlAggregation": "pages",
                "bypassed": {
                    "target": [
                        "notFound"
                    ]
                },
                "async": true
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "login",
                    "target": [
                        "login"
                    ]
                },
                {
                    "pattern": "Home",
                    "name": "home",
                    "target": [
                        "home"
                    ]
                },

                {
                    "pattern": "Tenants",
                    "name": "tenants",
                    "target": [
                        "tenants"
                    ]
                },
                {
                    "pattern": "TenantDetail/{mode}/{id}/:systemId:",
                    "name": "tenantDetail",
                    "target": [
                        "tenantDetail"
                    ]
                }
            ],
            "targets": {
                "app": {
                    "viewName": "App"
                },

                "login": {
                    "viewName": "Login",
                    "viewId": "LoginView"
                },
                "home": {
                    "viewName": "Home",
                    "viewId": "HomeView",
                    "parent": "app",
                    "controlId": "adminApp",
                    "controlAggregation": "pages"
                },
                "tenants": {
                    "viewName": "TenantList",
                    "viewId": "TenantListView",
                    "viewPath": "vcs.view.tenants.tenantList",
                    "parent": "app",
                    "controlId": "adminApp",
                    "controlAggregation": "pages"
                },
                "tenantDetail": {
                    "viewName": "TenantDetail",
                    "viewId": "TenantDetailView",
                    "viewPath": "vcs.view.tenants.tenantDetail",
                    "parent": "app",
                    "controlId": "adminApp",
                    "controlAggregation": "pages"
                }
            }
        }
    }
}